{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "NodeNext",
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "strict": true,
        "outDir": "dist"
    },
    "include": ["./src"],
    "typedocOptions": {
        "entryPoints": "./src",
        "entryPointStrategy": "expand",
        "excludePrivate": true
    }
}
